unit untClasseCliente;

interface

type
  TPessoa = class
    cpf: integer;
    nome: string;
    procedure setNome( n : string );
    function getNome : String;
  end;

implementation

//Método para atribuir valores a propriedade (atributo) "nome"
procedure TPessoa.setNome( n : string );
begin
  self.nome := n;
end;

//Método
function TPessoa.getNome : string;
begin
  getNome := self.nome
end;

end.
